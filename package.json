{
  "name": "mobile-dev-boilerplate-inferno",
  "version": "1.0.0",
  "description": "",
  "main": "webpack/development.config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "android": "npm run build && cordova prepare android && cordova run android --device",
    "build": "webpack --config webpack/production.config.js --progress -p",
    "clean:windows": "rmdir /s /q ./www",
    "start": "webpack-dev-server --config webpack/development.config.js --inline",
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js \"test/**/*.@(js|jsx)\"",
    "test:watch": "npm run test -- --watch --watch-extensions jsx"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "es2015",
          "stage-0",
          "stage-1",
          "stage-2"
        ]
      }
    },
    "plugins": [
      "babel-plugin-syntax-jsx",
      "babel-plugin-inferno"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-inferno": "1.0.0-legacy",
    "babel-plugin-syntax-jsx": "6.13.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "babel-preset-stage-2": "6.17.0",
    "browser-sync": "2.17.5",
    "browser-sync-webpack-plugin": "1.1.3",
    "chai": "3.5.0",
    "chai-immutable": "1.6.0",
    "clean-webpack-plugin": "0.1.13",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "3.0.0",
    "jsdom": "9.8.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "mocha": "3.1.2",
    "node-sass": "3.10.1",
    "postcss-loader": "1.0.0",
    "precss": "1.4.0",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2",
    "webpack-livereload-plugin": "0.9.0"
  },
  "dependencies": {
    "cordova-plugin-background-mode": "0.6.5",
    "cordova-plugin-crosswalk-webview": "2.1.0",
    "cordova-plugin-device": "1.1.3",
    "cordova-plugin-google-analytics": "1.6.0",
    "cordova-plugin-splashscreen": "4.0.0",
    "font-awesome": "4.6.3",
    "framework7": "1.4.2",
    "immutable": "3.8.1",
    "inferno": "0.7.27",
    "inferno-component": "0.7.27",
    "inferno-create-element": "0.7.27",
    "inferno-dom": "0.7.27",
    "ionicons": "3.0.0",
    "mappersmith": "0.13.3",
    "moment": "2.15.1",
    "numbro": "1.9.3"
  }
}
